# Docker Compose environment configuration
# Copy to .env and customize

# Image settings
IMAGE_NAME=devai-lab
IMAGE_TAG=latest

# Port mappings
PORT=8888
OLLAMA_PORT=11434

# Ollama host (use service name in compose, override for external)
OLLAMA_HOST=http://ollama:11434

# Working directory mount
HOST_WORK_DIR=./work

# User mapping (for rootless containers)
USER_ID=1000
GROUP_ID=1000
CONTAINER_USER=devai

# Proxy settings (optional)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080

# API Keys (keep empty here, set in environment or secrets)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
