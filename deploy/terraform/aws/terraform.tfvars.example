# AWS Terraform variables
# Copy to terraform.tfvars and customize

# Required
region = "us-east-1"

# Project settings
project_name = "devai-lab"
environment  = "dev"

# Resources
cpu             = 2
memory          = 4096
storage_size_gb = 50
replicas        = 1

# Features
enable_gpu   = false
enable_https = false

# Network
allowed_cidrs = ["0.0.0.0/0"]
vpc_cidr      = "10.0.0.0/16"

# Container
image_tag = "latest"
